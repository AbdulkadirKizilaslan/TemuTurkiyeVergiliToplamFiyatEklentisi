{
  "manifest_version": 3,
  "name": "Temu Türkye Vergili Fiyat Hesaplama Eklentisi",
  "version": "1.0",
  "description": "Temu'daki fiyatlara %60 vergi koyar ve size gerçek fiyatı gösterir.",
  "permissions": ["storage"],
  "host_permissions": ["https://www.temu.com/*"],
  "content_scripts": [
    {
      "matches": ["https://www.temu.com/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
     "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}



